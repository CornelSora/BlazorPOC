@page "/counter"

<h1>Counter</h1>

<p>Current count: @currentCount</p>
<section>
    <VueMessage @ref="vueMessage" />
    <div class="col-10 row">
        <button class="btn btn-primary" @onclick="IncrementCount">Click me</button>
    </div>
    <Map Zoom="10" Markers="MapMarkers"></Map>
</section>